%p#notice= notice
%h1 Listing apps
%table
  %thead
    %tr
      %th User
      %th Program
      %th{:colspan => "3"}
  %tbody
    - @apps.each do |app|
      %tr
        %td= app.user.email
        %td= app.program_id
        %td= link_to 'Show', app
        %td= link_to 'Edit', edit_app_path(app)
        %td= link_to 'Destroy', app, method: :delete, data: { confirm: 'Are you sure?' }
%br
  = link_to 'New app', new_app_path
